    <link name='left_wheel'>
      <!-- base_link 기준 위치: 좌측으로 base_width/2, 약간 아래/앞 (추측입니다.) -->
      <!-- base_width를 slotcar 플러그인에서 0.43으로 썼으니 0.215 = 0.43 / 2 -->
      <pose relative_to='base_link'>0 0.215 0.10 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>3.0</mass>          <!-- 추측입니다. 한 축에 달린 두 휠 총 질량 가정 -->
        <inertia>                 <!-- 추측입니다. 대충 대칭값 -->
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.02</izz>
        </inertia>
      </inertial>
      <!-- 앞쪽 왼쪽 바퀴: visual -->
      <visual name='left_wheel_front_visual'>
        <!-- left_wheel 링크 기준으로 앞쪽으로 0.20m (추측입니다.) -->
