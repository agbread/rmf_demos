        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://Go2W/meshes/left_wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <!-- 앞쪽 왼쪽 바퀴: collision -->
      <collision name='left_wheel_front_collision'>
        <pose>0.20 0 0 0 0 0</pose>   <!-- visual과 동일 위치 -->
        <geometry>
          <!-- 7 inch 타이어 → 반지름 0.089m (확실) -->
          <cylinder>
            <length>0.08</length>     <!-- 타이어 두께 대략 값, 추측입니다. -->
            <radius>0.089</radius>    <!-- 확실 -->
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <ode>
              <kp>100000.0</kp>       <!-- 기존 1e6보다 낮춘 값, 추측입니다. -->
              <kd>1.0</kd>
            </ode>
          </contact>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <!-- 뒤쪽 왼쪽 바퀴: collision -->
